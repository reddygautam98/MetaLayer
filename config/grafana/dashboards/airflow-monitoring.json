      "targets": [
        {
          "datasource": {
            "type": "postgres",
            "uid": "postgres"
          },
          "format": "time_series",
          "rawQuery": true,
          "rawSql": "SELECT \n  $__timeGroup(execution_date,'1h') as time,\n  state,\n  count(*) as value\nFROM dag_run \nWHERE $__timeFilter(execution_date)\nGROUP BY time, state\nORDER BY time;",
          "refId": "A"
        }
      ],
      "title": "DAG Run Trends",
      "type": "timeseries"
