      "targets": [
        {
          "datasource": {
            "type": "postgres",
            "uid": "postgres"
          },
          "format": "table",
          "rawQuery": true,
          "rawSql": "SELECT \n  CASE \n    WHEN schemaname = 'bronze' THEN 'Bronze Layer'\n    WHEN schemaname = 'silver' THEN 'Silver Layer'\n    WHEN schemaname = 'gold' THEN 'Gold Layer'\n  END as layer,\n  COUNT(*) as record_count \nFROM pg_tables \nWHERE schemaname IN ('bronze', 'silver', 'gold') \nGROUP BY schemaname \nORDER BY \n  CASE schemaname \n    WHEN 'bronze' THEN 1\n    WHEN 'silver' THEN 2\n    WHEN 'gold' THEN 3\n  END",
          "refId": "A"
        }
      ],
      "title": "Layer Record Counts",
      "type": "piechart"
    },
    {
      "datasource": {
        "type": "postgres",
        "uid": "postgres"
      },
      "fieldConfig": {
        "defaults": {
          "color": {
            "mode": "thresholds"
          },
          "custom": {
            "align": "auto",
            "displayMode": "auto",
            "inspect": false
          },
          "mappings": [],
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {
                "color": "green",
                "value": null
              },
              {
                "color": "red",
                "value": 1
              }
            ]
          }
        },
        "overrides": [
          {
            "matcher": {
              "id": "byName",
              "options": "state"
            },
            "properties": [
              {
                "id": "custom.displayMode",
                "value": "color-background"
              },
              {
                "id": "mappings",
                "value": [
                  {
                    "options": {
                      "success": {
                        "color": "green",
                        "index": 0
                      },
                      "failed": {
                        "color": "red",
                        "index": 1
                      },
                      "running": {
                        "color": "blue",
                        "index": 2
                      },
                      "queued": {
                        "color": "yellow",
                        "index": 3
                      }
                    },
                    "type": "value"
                  }
                ]
              }
            ]
          }
        ]
      },
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 12,
        "y": 0
      },
      "id": 2,
      "options": {
        "showHeader": true
      },
      "targets": [
        {
          "datasource": {
            "type": "postgres",
            "uid": "postgres"
          },
          "format": "table",
          "rawQuery": true,
          "rawSql": "SELECT \n  dag_id,\n  state,\n  COUNT(*) as runs,\n  MAX(execution_date) as last_execution\nFROM dag_run \nWHERE execution_date >= NOW() - INTERVAL '24 hours'\nGROUP BY dag_id, state\nORDER BY last_execution DESC",
          "refId": "A"
        }
      ],
      "title": "Database Schemas Overview",
      "type": "table"
    },
